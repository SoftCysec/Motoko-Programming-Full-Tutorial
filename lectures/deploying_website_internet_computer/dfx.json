{
  "canisters": {
    "backend": {
      "main": "canisters/backend/main.mo",
      "type": "motoko"
    },
    "frontend": {
      "frontend": {
        "entrypoint": "index.html"
      },
      "source": ["dist/"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://mainnet.dfinity.network"],
      "type": "persistent"
    }
  },
  "version": 1
}
